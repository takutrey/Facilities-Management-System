<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>EstateAgency Bootstrap Template - Index</title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="/assets/img/favicon.png" rel="icon">
  <link href="/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="/assets/vendor/animate.css/animate.min.css" rel="stylesheet">
  <link href="/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="/assets/css/style.css" rel="stylesheet">

  <!-- =======================================================
  * Template Name: EstateAgency - v4.8.0
  * Template URL: https://bootstrapmade.com/real-estate-agency-bootstrap-template/
  * Author: BootstrapMade.com
  * License: https://bootstrapmade.com/license/
  ======================================================== -->
</head>

<body>


<%- include('partials/head') %> 

  <main id="main">


    <!-- ======= Intro Single ======= -->
    <% singleproperties.forEach(function(singleproperty){ %>

    <section class="intro-single">
      <div class="container">
        <div class="row">
          <div class="col-md-12 col-lg-8">
            <div class="title-single-box">
              <h1 class="title-single"><%= singleproperty.address%></h1>
              <span class="color-text-a"><%= singleproperty.location%></span>
            </div>
          </div>
        </div>
      </div>
    </section><!-- End Intro Single-->

    <!-- ======= Property Single ======= -->

    <section class="property-single nav-arrow-b"> 
      
      <%- include('partials/imageswiper') %>  

      <div class="container"> 

        <div class="row">
          <div class="col-sm-12">

            <div class="row justify-content-between">
              <div class="col-md-5 col-lg-4">
                <div class="property-price d-flex justify-content-center foo">
                  <div class="card-header-c d-flex">
                    <div class="card-box-ico">
                      <span class="bi bi-cash">$</span>
                    </div>
                    <div class="card-title-c align-self-center">
                      <h5 class="title-c"><%= singleproperty.price%></h5>
                    </div>
                  </div>
                </div>
                <div class="property-summary">
                  <div class="row">
                    <div class="col-sm-12">
                      <div class="title-box-d section-t4">
                        <h3 class="title-d">Quick Summary</h3>
                      </div>
                    </div>
                  </div>
                  <div class="summary-list">
                    <ul class="list">
                    
                      <li class="d-flex justify-content-between">
                        <strong>Location:</strong>
                        <span><%= singleproperty.location%></span>
                      </li>
                      <li class="d-flex justify-content-between">
                        <strong>Property Address:</strong>
                        <span><%= singleproperty.address%></span>
                      </li> 
                      <li class="d-flex justify-content-between">
                        <strong>Property Type:</strong>
                        <span><%= singleproperty.propertytype%></span>
                      </li>
                      <li class="d-flex justify-content-between">
                        <strong>Insurance:</strong>
                        <span><%= singleproperty.insurance%></span>
                      </li> 
                      
                     
                      <li class="d-flex justify-content-between">
                        <strong>Area:</strong>
                        <span><%= singleproperty.area%>m
                          <sup>2</sup>
                        </span>
                      </li> 
                      <li class="d-flex justify-content-between">
                        <strong>Number of Rooms:</strong>
                        <span><%= singleproperty.rooms%></span>
                      </li>
                      <li class="d-flex justify-content-between">
                        <strong>Bedrooms:</strong>
                        <span><%= singleproperty.bedrooms%></span>
                      </li>
                      <li class="d-flex justify-content-between">
                        <strong>Bathrooms:</strong>
                        <span><%= singleproperty.bathrooms%></span>
                      </li>
                     
                    </ul>
                  </div>
                </div>
              </div>
              <div class="col-md-7 col-lg-7 section-md-t3">
                <div class="row">
                  <div class="col-sm-12">
                    <div class="title-box-d">
                      <h3 class="title-d">Property Description</h3>
                    </div>
                  </div>
                </div>
                <div class="property-description">
                  <p class="description color-text-a no-margin">
                    <%= singleproperty.vacdescription%>
                  </p>
                 
                </div>
                <div class="row section-t3">
                  <div class="col-sm-12">
                    <div class="title-box-d">
                      <h3 class="title-d">Amenities</h3>
                    </div>
                  </div>
                </div>
                <div class="amenities-list color-text-a">
                  <%= singleproperty.amenities%>
                </div>
              </div>
            </div>
          </div>
          <div class="col-md-10 offset-md-1">
            <ul class="nav nav-pills-a nav-pills mb-3 section-t3" id="pills-tab" role="tablist">
              <li class="nav-item">
                <a class="nav-link active" id="pills-video-tab" data-bs-toggle="pill" href="#pills-video" role="tab" aria-controls="pills-video" aria-selected="true">Video</a>
              </li>
             
            
            <div class="tab-content" id="pills-tabContent">
              <% for(const item of video) { %>

              <div class="tab-pane fade show active" id="pills-video" role="tabpanel" aria-labelledby="pills-video-tab">
                  <video width="80%" height="460" controls>
                    <source src="/<%= item.videos %> " type="video/mp4">
                    Your browser does not support the video tag.
                  </video>

              </div> 
              <% } %>

            </div> 
            <div class="col-md-12 text-center">
              <a href="https://wa.me/+263<%= singleproperty.phone%>"type="submit" value="submit" class="btn btn-a">Send Enquiry</a>
            </div>
          </div> 

          <div class="col-md-12">
            <div class="row section-t3">
              <div class="col-sm-12">
                <div class="title-box-d">
                  <h3 class="title-d">Contact Agent</h3>
                </div>
              </div>
            </div>  


            <div class="row">
              <div class="col-md-6 col-lg-4">
                <img src="/<%= singleproperty.profileimage %>" alt="" class="img-fluid">
              </div> 

              <div class="col-md-6 col-lg-4">
                <div class="property-agent"> 

                  <h4 class="title-agent">  <%= singleproperty.agentFullName%> </h4>  



                  <p class="color-text-a">
                    <%= singleproperty.description%></p>
                  <ul class="list-unstyled">
                    <li class="d-flex justify-content-between">
                      <strong>Phone:</strong>
                      <span class="color-text-a">                    <%= singleproperty.phone%>
                      </span>
                    </li>
                   
                    <li class="d-flex justify-content-between">
                      <strong>Email:</strong>
                      <span class="color-text-a">                    <%= singleproperty.email%>
                      </span>
                    </li>
                   
                  </ul>
                  <div class="socials-a">
                    <ul class="list-inline">
                      <li class="list-inline-item">
                        <a href=" <%= singleproperty.facebook%>">
                          <i class="bi bi-facebook" aria-hidden="true"></i>
                        </a>
                      </li>
                      <li class="list-inline-item">
                        <a href=" <%= singleproperty.twitter%>">
                          <i class="bi bi-twitter" aria-hidden="true"></i>
                        </a>
                      </li>
                      <li class="list-inline-item">
                        <a href=" <%= singleproperty.instagram%>">
                          <i class="bi bi-instagram" aria-hidden="true"></i>
                        </a>
                      </li>
                     
                    </ul>
                  </div>
                  <% }); %>  

                </div>

              </div> 

            </div> 

          </div>
        </div>
      </div>
    </section><!-- End Property Single-->

  </main><!-- End #main -->

  <%- include('partials/footer') %> 
  <!-- Vendor JS Files --> 
  <script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js'></script>
  <script src='https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js'></script><script  src="../swiper.js"></script>
  <script src="/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/assets/vendor/swiper/swiper-bundle.min.js"></script>
  <script src="/assets/vendor/php-email-form/validate.js"></script> 

  <!-- Template Main JS File -->
  <script src="/assets/js/main.js"></script> 

 


</body>

</html>