<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Purple Admin</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="/assets/vendors/css/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- Plugin css for this page -->
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <!-- endinject -->
    <!-- Layout styles -->
    <link rel="stylesheet" href="/assets/css/stylee.css">
    <!-- End layout styles -->
    <link rel="shortcut icon" href="/images/images.png" />
  </head>
  <body>  

    <%- include('partials/dashnavbar') %>  

    <%- include('partials/dashsidebar') %>  



    <div class="col-md-6 grid-margin stretch-card">
      <div class="card">
        <div class="card-body">
          <h4 class="card-title"><%= title %> </h4> 

          <form class="forms-sample" action="/updatehouse" method="post"> 

            <input type="hidden" name="vacId"  value="<%= agentprop.vacId %>">



            <div class="form-group row">
              <label for="address" class="col-sm-3 col-form-label">Address</label>
              <div class="col-sm-9">
                <input type="text" class="form-control" name="address" id="address" value="<%= agentprop.address %>" placeholder="Enter property address">
              </div>
            </div>  


            <div class="form-group row">
              <label for="location" class="col-sm-3 col-form-label">Location</label>
              <div class="col-sm-9">
                <input type="text" class="form-control" name="location"id="location" value="<%= agentprop.location%>" placeholder="Harare, Mutare, Marondera etc">
              </div>
            </div>  
              
            <div class="form-group row">
              <label for="tenant" class="col-sm-3 col-form-label">Tenant</label>
              <div class="col-sm-9">
                <input type="text" class="form-control" name="tenant" id="tenant" value="<%= agentprop.tenant%>" placeholder="Enter tenant of residence">
              </div>
            </div>  

            <div class="form-group row">
              <label for="area" class="col-sm-3 col-form-label">Area of property</label>
              <div class="col-sm-9">
                <input type="number" class="form-control"name="area" id="area" value="<%= agentprop.area%>" placeholder="area">
              </div>
            </div>
            <div class="form-group row">
              <label for="rooms" class="col-sm-3 col-form-label">Total number of rooms</label>
              <div class="col-sm-9">
                <input type="number" class="form-control" name="rooms" id="rooms" value="<%= agentprop.rooms %>" placeholder="Enter total rooms">
              </div>
            </div>
            <div class="form-group row">
              <label for="bedrooms" class="col-sm-3 col-form-label">Number of bedrooms</label>
              <div class="col-sm-9">
                <input type="number" class="form-control" name="bedrooms" id="bedrooms" value="<%= agentprop.bedrooms%>" placeholder="Enter number of bedrooms">
              </div>
            </div>
            <div class="form-group row">
              <label for="bathrooms" class="col-sm-3 col-form-label">Number of bathrooms</label>
              <div class="col-sm-9">
                <input type="number" class="form-control"name="bathrooms" id="bathrooms" value="<%= agentprop.bathrooms%>" placeholder="Enter number of bathrooms">
              </div>
            </div>  

            <div class="form-group row">
              <label for="amenities" class="col-sm-3 col-form-label">Amenities</label>
              <div class="col-sm-9">
                <input type="text" class="form-control" name="amenities"id="amenities" value="<%= agentprop.amenities%>" placeholder="Garage, WiFi, Durawall etc">
              </div>
            </div>    

            

            <div class="form-group row">
              <label for="price" class="col-sm-3 col-form-label">Price</label>
              <div class="col-sm-9">
                <input type="number" class="form-control" name="price" id="price" value="<%= agentprop.price%>" placeholder="Enter price">
              </div>
            </div> 

            <div class="form-group row">
              <label for="insurance" class="col-sm-3 col-form-label">Insurance Policy</label>
              <div class="col-sm-9">
                <input type="text" class="form-control" name="insurance" value="<%= agentprop.insurance%>" id="insurance" placeholder="Enter Insurance Company">
              </div>
            </div> 

            <div class="form-group">
              <label for="description" class="col-sm-3 col-form-label">House Description</label>
              <textarea name="description" cols="40" rows="5"></textarea>
            </div>    
            
            <button type="submit" class="btn btn-gradient-primary me-2">Submit</button>
          </form>
        </div>
      </div>
    </div>
       
       

    <!-- plugins:js -->
    <script src="/assets/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="/assets/vendors/chart.js/Chart.min.js"></script>
    <script src="/assets/js/jquery.cookie.js" type="text/javascript"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="/assets/js/off-canvas.js"></script>
    <script src="/assets/js/hoverable-collapse.js"></script>
    <script src="/assets/js/misc.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="/assets/js/dashboard.js"></script>
    <script src="/assets/js/todolist.js"></script>
    <!-- End custom js for this page -->
  </body>
</html>