<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Purple Admin</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="assets/vendors/css/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- Plugin css for this page -->
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <!-- endinject -->
    <!-- Layout styles -->
    <link rel="stylesheet" href="assets/css/stylee.css">
    <!-- End layout styles -->
    <link rel="shortcut icon" href="images/images.png" />
  </head>
  <body>
        
        <!-- start navbar partial -->
        <%- include('partials/dashnavbar') %>  
        <!--end nav bar partial--> 

        <!-- start sidebar partial -->
        <%- include('partials/dashsidebar') %>  
        <!--end sidebar partial-->

        <div class="col-lg-8 grid-margin stretch-card">
            <div class="card">
              <div class="card-body">
                <h2 class="card-title">My Properties</h2> 
                <a href="/addhouse" class="btn btn-primary">Add Details</a>

                
                <table class="table table-striped">
                  <thead>
                    <tr>
                      <th> Address </th>
                      <th> Location </th>
                      <th> Total Rooms </th>
                      <th> Bedrooms </th>
                      <th> Bathrooms </th> 
                      <th> Price </th> 
                      <th>Tenant</th> 
                      <th>Actions</th>
                      

                    </tr>
                  </thead>
                  <tbody> 

                    <% myproperties.forEach(function(agentprop){ %>
                      <tr>
                        <td><%= agentprop.address %> </td> 
                        <td><%= agentprop.location%> </td> 
                        <td><%= agentprop.rooms%> </td> 
                        <td><%= agentprop.bedrooms%> </td> 
                        <td><%= agentprop.bathrooms %> </td> 
                        <td><%= agentprop.price %> </td>  
                        <td><%= agentprop.tenant %> </td>  

                        <td> 

                         
                          <a class="btn btn-info btn-sm" href="addvideo/<%= agentprop.vacId %>"> <i class="mdi mdi-video"></i></a> 
                          <a class="btn btn-warning btn-sm edit" href="alter/<%= agentprop.vacId %>"> <i class="mdi mdi-border-color"></i></a>
                          <a class="btn btn-danger btn-sm delete" href="remove/<%= agentprop.vacId %>"><i class="mdi mdi-delete"></i></i>
                          </a> 

                        </td>
                      </tr>
                 <% }); %>  

                  </tbody>
                </table>
              </div>
            </div>
          </div>


    <!-- plugins:js -->
    <script src="assets/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="assets/vendors/chart.js/Chart.min.js"></script>
    <script src="assets/js/jquery.cookie.js" type="text/javascript"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="assets/js/off-canvas.js"></script>
    <script src="assets/js/hoverable-collapse.js"></script>
    <script src="assets/js/misc.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="assets/js/dashboard.js"></script>
    <script src="assets/js/todolist.js"></script>
    <!-- End custom js for this page -->
  </body>
</html>